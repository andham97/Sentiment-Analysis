{
  "name": "sentiment-analysis",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node build/server/server.js",
    "pretest": "eslint . --ext .js --ignore-path .gitignore",
    "test": "jest",
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel -d ./build/server ./src/server",
    "build-client": "webpack",
    "build": "npm run clean && npm run build-server && npm run build-client",
    "debug-server": "nodemon --exec babel-node src/server/server.js",
    "debug-client": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andham97/Sentiment-Analysis.git"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": []
  },
  "author": "Andreas Hammer, Dina Rosvoll, Oda Steinland Skaug",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "path": "^0.12.7",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "plugin:jest/recommended",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "jest/globals": true,
      "browser": true
    },
    "plugins": [
      "jest",
      "react"
    ],
    "rules": {
      "class-methods-use-this": 0
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "eslintIgnore": [
    "node_modules"
  ],
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/enzyme.config.js"
  }
}
