{
  "engines": {
    "node": "9.3.0",
    "npm": "6.5.0"
  },
  "name": "sentiment-analysis",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node build/server/server.js",
    "pretest": "eslint . --ext .js --ignore-path .gitignore",
    "test": "jest",
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel -d ./build/server ./src/server",
    "build-client": "webpack",
    "build": "npm run clean && npm run build-server && npm run build-client",
    "debug-server": "nodemon --exec babel-node src/server/server.js",
    "debug-client": "webpack --watch",
    "debug-ws": "nodemon --exec babel-node src/server/ws/webscraper.js",
    "ws": "babel-node src/server/ws/webscraper.js",
    "prod-ws": "node build/server/ws/webscraper.js",
    "nf": "babel-node src/server/news-fetch.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andham97/Sentiment-Analysis.git"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": []
  },
  "author": "Andreas Hammer, Dina Rosvoll, Oda Steinland Skaug",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "@cloudant/cloudant": "^3.0.2",
    "cfenv": "^1.1.0",
    "cheerio": "^1.0.0-rc.2",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "newsapi": "^2.4.0",
    "react": "^16.7.0",
    "react-autocomplete": "^1.8.1",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-bootstrap-typeahead": "^4.0.0-alpha.5",
    "react-d3-cloud": "^0.7.0",
    "react-datepicker": "^2.2.0",
    "react-dates": "^20.1.0",
    "react-dom": "^16.7.0",
    "react-icons": "^3.5.0",
    "react-minimal-pie-chart": "^3.5.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-tagcloud": "^1.4.0",
    "request-promise": "^4.2.4",
    "validator": "^10.11.0",
    "watson-developer-cloud": "^3.17.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.1.0",
    "nodemon": "^1.18.9",
    "path": "^0.12.7",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "plugin:jest/recommended",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "jest/globals": true,
      "browser": true
    },
    "plugins": [
      "jest",
      "react"
    ],
    "rules": {
      "class-methods-use-this": 0,
      "prefer-destructuring": 0,
      "brace-style": [
        2,
        "stroustrup"
      ],
      "consistent-return": 0,
      "nonblock-statement-body-position": 0,
      "curly": 0,
      "no-console": 0,
      "no-param-reassign": 0,
      "no-shadow": [
        "error",
        {
          "allow": [
            "err"
          ]
        }
      ]
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "eslintIgnore": [
    "node_modules"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/enzyme.config.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
