{
  "engines": {
    "node": "9.3.0",
    "npm": "6.5.0"
  },
  "name": "sentiment-analysis",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node build/server/server.js",
    "pretest": "eslint ./src --ext .js --ignore-path .gitignore && rm -rf coverage",
    "test": "echo $PWD && ls && jest --config jest.conf.json",
    "posttest": "codecov",
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel -d ./build/server ./src/server",
    "build-client": "webpack",
    "build": "npm run clean && npm run build-server && npm run build-client",
    "debug-server": "nodemon --exec babel-node src/server/server.js --watch src/server",
    "debug-client": "webpack --watch",
    "debug-ws": "nodemon --exec babel-node src/server/ws/webscraper.js",
    "ws": "babel-node src/server/ws/webscraper.js",
    "prod-ws": "node build/server/ws/webscraper.js",
    "jsdoc": "nodemon --exec npm run jd --watch src/server",
    "esdoc": "nodemon --exec npm run ed --watch src/client",
    "jd": "rm -rf docs/server && documentation build src/server/** -f html -o docs/server",
    "ed": "rm -rf docs/client && esdoc",
    "try": "nodemon --exec babel-node src/server/tst.js --watch src/server",
    "testing": "nodemon --exec npm test --watch test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andham97/Sentiment-Analysis.git"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": []
  },
  "author": "Andreas Hammer, Dina Rosvoll, Oda Steinland Skaug",
  "license": "MIT",
  "dependencies": {
    "@cloudant/cloudant": "^3.0.2",
    "@react-pdf/renderer": "^1.5.4",
    "auth0": "^2.16.0",
    "bootstrap": "^4.3.1",
    "brace": "^0.11.1",
    "cfenv": "^1.2.1",
    "cheerio": "^1.0.0-rc.2",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-sanitizer": "^1.0.5",
    "express-session": "^1.15.6",
    "helmet": "^3.18.0",
    "html2canvas": "^1.0.0-rc.1",
    "js-beautify": "^1.9.1",
    "jspdf": "^1.5.3",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "newsapi": "^2.4.0",
    "passport": "^0.4.0",
    "passport-auth0": "^1.1.0",
    "prismjs": "^1.16.0",
    "prop-types": "^15.7.2",
    "rasterizehtml": "^1.3.0",
    "react": "^16.7.0",
    "react-ace": "^6.4.0",
    "react-autocomplete": "^1.8.1",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-bootstrap-typeahead": "^4.0.0-alpha.5",
    "react-d3-cloud": "^0.7.0",
    "react-datepicker": "^2.3.0",
    "react-dates": "^20.1.0",
    "react-dom": "^16.7.0",
    "react-hotkeys": "^1.1.4",
    "react-icons": "^3.5.0",
    "react-minimal-pie-chart": "^3.5.0",
    "react-onclickoutside": "^6.8.0",
    "react-responsive": "^6.1.1",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-s-alert": "^1.4.1",
    "react-simple-code-editor": "^0.9.10",
    "react-tagcloud": "^1.4.0",
    "recharts": "^1.5.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "search-text-tokenizer": "^2.1.1",
    "url-loader": "^1.1.2",
    "uuid": "^3.3.2",
    "validator": "^10.11.0",
    "watson-developer-cloud": "^3.17.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "codecov": "^3.5.0",
    "css-loader": "^2.1.0",
    "documentation": "^11.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-react-plugin": "^1.0.1",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-jsdoc": "^4.8.4",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.1.0",
    "jsdoc": "^3.6.2",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-route-plugin": "^0.1.0",
    "jsdoc-to-markdown": "^5.0.0",
    "loke-jsdoc-theme": "^2.1.0",
    "nock": "^10.0.6",
    "nodemon": "^1.18.9",
    "path": "^0.12.7",
    "style-loader": "^0.23.1",
    "supertest": "^4.0.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "plugin:jest/recommended",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "jest/globals": true,
      "browser": true
    },
    "plugins": [
      "jest",
      "react",
      "jsdoc"
    ],
    "rules": {
      "class-methods-use-this": 0,
      "prefer-destructuring": 0,
      "brace-style": [
        2,
        "stroustrup"
      ],
      "consistent-return": 0,
      "nonblock-statement-body-position": 0,
      "curly": 0,
      "no-console": 0,
      "no-param-reassign": 0,
      "no-shadow": [
        "error",
        {
          "allow": [
            "err"
          ]
        }
      ],
      "no-underscore-dangle": 0,
      "import/named": 0,
      "no-plusplus": 0,
      "prefer-promise-reject-errors": 0,
      "no-eval": 0,
      "no-await-in-loop": 0,
      "no-restricted-globals": 0,
      "import/no-unresolved": 0,
      "jsdoc/check-alignment": 1,
      "jsdoc/check-examples": 1,
      "jsdoc/check-indentation": 1,
      "jsdoc/check-param-names": 1,
      "jsdoc/check-syntax": 1,
      "jsdoc/check-tag-names": 1,
      "jsdoc/check-types": 1,
      "jsdoc/newline-after-description": 1,
      "jsdoc/require-hyphen-before-param-description": 1,
      "jsdoc/require-jsdoc": 1,
      "jsdoc/require-param": 1,
      "jsdoc/require-param-name": 1,
      "jsdoc/require-param-type": 1,
      "jsdoc/valid-types": 1
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "eslintIgnore": [
    "node_modules",
    "docs"
  ]
}
