{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": [ "src" ],
        "exclude": [ "node_modules" ],
        "includePattern": ".+\\.js(doc|x)?$"
    },
    "plugins": [
      "node_modules/jsdoc-babel",
      "node_modules/jsdoc-route-plugin"
    ],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
      "recurse": true,
      "template": "node_modules/loke-jsdoc-theme",
      "encoding": "utf-8"
    },
    "babel": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
      ],
      "plugins": []
    }
}
