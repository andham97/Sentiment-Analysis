{
  "setupFilesAfterEnv": [
    "<rootDir>/enzyme.config.js",
    "<rootDir>/testSetup.js"
  ],
  "moduleNameMapper": {
    "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js"
  },
  "globals": {
    "__DEV__": true
  },
  "coverageDirectory": "<rootDir>/coverage",
  "collectCoverageFrom": [
    "src/server/**/*.js"
  ],
  "collectCoverage": true,
  "resetMocks": true
}
